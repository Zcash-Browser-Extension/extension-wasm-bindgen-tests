{
    "name": "WASM-bindgen-tests",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "description": "A Chromium extension for wasm-bindgen tests",
    "permissions": [
        "http://*/", "https://*/"
        ],    
    "content_security_policy": "script-src 'self' 'wasm-eval'; object-src 'self'",
    "browser_action": {
        "default_popup": "popup/popup.html"
    }    
}